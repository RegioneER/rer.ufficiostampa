<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="rer.ufficiostampa">
<head>
<metal:style fill-slot="style_slot">
  <link tal:define="css python:view.get_resource_css(name='tableViews')"
        href="${css}" rel="stylesheet" />
</metal:style>
</head>

<body>
<metal:main fill-slot="content">
  <article id="content">
    <h1 class="documentFirstHeading" i18n:translate="channels_management_label">Channels management</h1>
    <div id="content-core">
      <div id="management-wrapper" data-authenticator="${view/get_token}" data-app-type="channels"/>
    </div>
    <script tal:define="js python:view.get_resource_js(name='tableViews')" src="${js}"></script>
  </article>
</metal:main>
</body>
</html>
