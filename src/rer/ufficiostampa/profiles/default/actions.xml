<object xmlns:i18n="http://xml.zope.org/namespaces/i18n" meta_type="Plone Actions Tool" name="portal_actions">
  <object name="object" purge="False">
    <object meta_type="CMF Action" name="ufficiostampa-send" i18n:domain="rer.ufficiostampa">
      <property name="title" i18n:translate="">Send comunicato</property>
      <property name="url_expr">string:${object_url}/@send-comunicato</property>
      <property name="link_target"></property>
      <property name="icon_expr"></property>
      <property name="available_expr">python: context.restrictedTraverse('@@show-send-links')()</property>
      <property name="permissions">
        <element value="rer.ufficiostampa: Send"></element>
      </property>
      <property name="visible">True</property>
    </object>
    <object meta_type="CMF Action" name="ufficiostampa-channels-management" i18n:domain="rer.ufficiostampa">
      <property name="title" i18n:translate="">Channels management</property>
      <property name="url_expr">string:${globals_view/navigationRootUrl}/controlpanel/ufficiostampa-managechannels</property>
      <property name="link_target"></property>
      <property name="icon_expr"></property>
      <property name="permissions">
        <element value="rer.ufficiostampa: Manage Channels"></element>
      </property>
      <property name="visible">True</property>
    </object>
    <object meta_type="CMF Action" name="ufficiostampa-history-management" i18n:domain="rer.ufficiostampa">
      <property name="title" i18n:translate="">History management</property>
      <property name="url_expr">string:${globals_view/navigationRootUrl}/controlpanel/ufficiostampa-managehistory</property>
      <property name="link_target"></property>
      <property name="icon_expr"></property>
      <property name="permissions">
        <element value="rer.ufficiostampa: Access History"></element>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
